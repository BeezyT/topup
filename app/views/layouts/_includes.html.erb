
<% if RAILS_ENV == "production" %>

  <script type="text/javascript">
    var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
    document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
  </script>
  <script type="text/javascript">
    try {
      var pageTracker = _gat._getTracker("UA-11323539-1");
      pageTracker._trackPageview();
    } catch(err) {}
  </script>

  <% if [nil, "jquery"].include? session[:library] %>
    <%= javascript_include_tag "/releases/latest/top_up-min", "/releases/seat_holder/latest/seat_holder-min", "defaults" %>
  <% else %>
    <%= javascript_include_tag "/releases/seat_holder/latest/seat_holder-min", :defaults, "top_up-pt" %>
  <% end %>

<% else %>

  <% if [nil, "jquery"].include? session[:library] %>
    <%= javascript_include_tag "development/top_up", "development/seat_holder", "defaults" %>
  <% else %>
    <%= javascript_include_tag "development/seat_holder", :defaults, "development/top_up-pt" %>
  <% end %>

<% end %>

<% if RAILS_ENV == "development" %>
	<%= stylesheet_link_tag "top_up/style", "top_up/dashboard/style", "top_up/quicklook/style" %>
	<!--[if IE]>
	  <%= stylesheet_link_tag "top_up/iefix" %>
	<![endif]-->
	<!--[if IE 6]>
	  <%= stylesheet_link_tag "top_up/dashboard/ie6fix", "top_up/quicklook/ie6fix" %>
	<![endif]-->
	<!--[if IE 7]>
	  <%= stylesheet_link_tag "top_up/dashboard/ie7fix" %>
	<![endif]-->
<% end %>

<%= stylesheet_link_tag "application" %>
<!--[if IE 6]>
  <%= stylesheet_link_tag "ie6fix" %>
<![endif]-->
<!--[if IE 7]>
  <%= stylesheet_link_tag "ie7fix" %>
